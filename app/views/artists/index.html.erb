<h1>Artist Index</h1>

<% @artists.each do |artist| %>
  <a href="/artists/<%= artist.id %>"><%= artist.name %></a>
  <%= button_to "Edit Artist", "/artists/#{artist.id}/edit", method: :get  %>
  <%= button_to "Delete Artist", "/artists/#{artist.id}", method: :delete  %>
  <br>
  <% end %>



  <%= link_to "Create a new artist", new_artist_path %>


  
    